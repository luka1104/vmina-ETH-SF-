datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Video {
  id         Int      @id @default(autoincrement())
  imagePaths String[]
  title      String
  details    String?
  price      Int
  txHash     String?
}
